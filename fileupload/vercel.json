{
    "builds" : [{
        "src" : "fileupload/wsgi.py",
        "use" : "@vercel/python",
        "config" : {"maxLambdaSize" : "15mb", "runtime" : "python3.11"}
    }],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "/"
        },
        {
            "src" : "/(.*)",
            "dest" : "/"
        }
    ]
}